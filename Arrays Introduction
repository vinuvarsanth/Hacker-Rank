#include<iostream>
using namespace std;
void rev(int arr[],int n);                         //{pre declaration of a function}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    } 
    rev(arr,n);
    return 0;
}      
void rev(int arr[],int n)
{
    for(int i=n-1;i>=0;i--)
    {
        cout<<arr[i]<<" ";
    }
}
